{
  "name": "setup-android",
  "version": "1.0.0",
  "private": true,
  "description": "setup android action",
  "main": "lib/main.js",
  "scripts": {
    "build": "tsx build.mts",
    "prettier": "prettier --write . && git add .",
    "prettier:ci": "prettier --check .",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.cjs,.mjs,.mts,.cts --max-warnings 0 --fix && git add .",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx,.cjs,.mjs,.mts,.cts --max-warnings 0",
    "typescript": "tsc --noEmit"
  },
  "engines": {
    "node": "18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daveol/setup-android.git"
  },
  "pre-commit": [
    "prettier",
    "lint",
    "typescript"
  ],
  "keywords": [
    "actions",
    "node",
    "setup",
    "android"
  ],
  "author": "daveol",
  "license": "ISC",
  "dependencies": {
    "@actions/cache": "3.2.2",
    "@actions/core": "1.10.0",
    "@actions/tool-cache": "2.0.1",
    "fs-extra": "11.1.1"
  },
  "devDependencies": {
    "@types/fs-extra": "11.0.1",
    "@types/node": "16.18.46",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "esbuild": "0.19.2",
    "eslint": "8.52.0",
    "eslint-plugin-prettier": "5.0.0",
    "js-yaml": "4.1.0",
    "pre-commit": "1.2.2",
    "prettier": "3.0.3",
    "tsx": "3.12.7",
    "typescript": "5.2.2"
  }
}
